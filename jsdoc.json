{
	"source": {
		"include": ["app.js", "app.log","getAuthority.js","transporter.js"],
		"exclude": [
			"node_modules",
			".gitignore",
			"package-lock.json",
			"package.json",
			"README.md",
			"jsdoc.json",
			"image.png",
			".env"
		],

		"includePattern": ".+\\.js(doc|x)?$",
		"excludePattern": "(^|\\/|\\\\)_"
	},
	"opts": {
		"destination": "./docs",
		"recurse": true,
		"template": "node_modules/clean-jsdoc-theme"
	}
}
